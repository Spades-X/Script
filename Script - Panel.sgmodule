#!name=Script - Panel
#!desc=Panel,自定义一个或多个信息面板显示相关的信息 @Spades-X


[Panel]
# PanelA = title=已完成订阅制解锁, content=感谢您回到Surge\n尊贵的Surge Pro用户, style=good

# Surge Pro标题,可显示启动时间,点击刷新为重载配置
SurgePro_ReloadProfile = script-name=SurgePro_ReloadProfile, update-interval=1

# 节假日查询
timecard = script-name=timecard, update-interval=3600

# 电信流量统计 
ChinaTelecom = script-name=ChinaTelecom, update-interval=3600

# 新冠疫情查询
# covid19 = script-name=covid19, title=新冠疫情查询, content=请刷新, update-interval=600

# YouTube Premium 查询
youtube_premium_check = script-name=youtube_premium_check, title="YouTube Premium 解锁检测", update-interval=3600

# Netflix 解锁查询
nf_check = script-name=nf_check, title="Netflix 解锁检测", update-interval=1
# NetflixSelect = script-name=NetflixSelect, update-interval=3600

# Disney+ 解锁查询
disney_check = script-name=disney_check, title=Disney+ 解锁检测, style=alert, content=请刷新面板, update-interval=1800

# 流量统计 点击以切换网络界面
# TrafficStatistics = script-name=TrafficStatistics, update-interval=1

# Surge 网络信息面板 
# net-info-panel = title="网络状态", content="请刷新", style=info, script-name=net-info-panel, update-interval=1
NetInfoPanel = title="网络状态", content="请刷新", style=info, script-name=net-info-panel.js, update-interval=1

# Surge 「DNS 结果」
# flushDNS = script-name=flushDNS, update-interval=1

# 机场流量信息（Nexitally）
Sub_info = script-name=Sub_info, update-interval=43200

# 机场流量信息（ARK）
Sub_info = script-name=Sub_info_01, update-interval=43200


[Script]
# 附带重载配置,Surge Pro标题,可显示启动时间,点击刷新为重载配置
SurgePro_ReloadProfile = type=generic, timeout=10, script-path= https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/surgepro_reloadprofile.js ,argument=icon=crown.fill&color=#f6c970

# 节假日提醒
timecard = type=generic, timeout=10, script-path=https://raw.githubusercontent.com/smartmimi/conf/master/surge/timecard.js

# 电信流量统计
ChinaTelecom = type=generic, script-path=https://raw.githubusercontent.com/mieqq/mieqq/master/telecom.js, pattern=https://e.189.cn/store/user/package_detail.do, requires-body=1, max-size=0, timeout=10

# 新冠疫情查询
# covid19 = type=generic, script-path=https://raw.githubusercontent.com/Spades-X/Script/main/Covid19.js

# YouTube Premium 查询
youtube_premium_check = type=generic, script-path=https://gist.githubusercontent.com/Hyseen/5ae36a6a5cb5690b1f2bff4aa19c766f/raw/youtube_premium_check.js?version=1632728073, timeout=30

# Netflix 解锁查询
nf_check = type=generic, script-path=https://gist.githubusercontent.com/Hyseen/b06e911a41036ebc36acf04ddebe7b9a/raw/nf_check.js, timeout=30
# NetflixSelect = type=generic, script-path=https://raw.githubusercontent.com/fishingworld/something/main/NetflixSelect/nf_autoselect.js, argument=icon1=checkmark.circle&color1=#55ba94&icon2=checkmark.circle.trianglebadge.exclamationmark&color2=#9a9ced&icon3=hand.raised.circle&color3=#ea5532&netflixGroup=📺 𝑁𝑒𝑡𝑓𝑙𝑖𝑥
# NetflixChecker = type=cron, cronexp=5 4 * * *, wake-system=1, timeout=3600, script-path=https://raw.githubusercontent.com/fishingworld/something/main/NetflixSelect/nf_autocheck.js, script-update-interval=0, control-api=1

# Disney+ 解锁查询
disney_check = type=generic, script-path=https://gist.githubusercontent.com/Hyseen/729fc4c3ac28c3f2e7c4a832d81780f3/raw/disney_check.js

# 流量统计 点击以切换网络界面
# TrafficStatistics = type=generic, timeout=10, script-path=https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/trafficstatistics.js, argument=icon=arrow.up.arrow.down.circle&color=#5d84f8

# Surge 网络信息面板
# net-info-panel = script-path=https://raw.githubusercontent.com/Peng-YM/QuanX/master/Tools/Panels/NetInfo/net-info-panel.js, type=generic
net-info-panel.js = script-path=https://raw.githubusercontent.com/Nebulosa-Cat/Surge/main/Panel/Network-Info/cn/networkCheck_CN.js, type=generic

# Surge 「DNS 结果」
# flushDNS = type=generic, timeout=10, script-path=https://raw.githubusercontent.com/zZPiglet/Task/master/asset/flushDNS.js, argument=icon=scale.3d&color=#FF3380

# 机场流量信息（Nexitally）
Sub_info = type = generic, timeout=10, script-path=https://raw.githubusercontent.com/mieqq/mieqq/master/sub_info_panel.js, script-update-interval=0, argument=url=https%3A%2F%2Fwww.greenhouseplaza.com%2FdownloadConfig%2FNodeList.aspx%3Furk%3D4e97a26c-460b-4166-a06c-341a9212fc04&reset_day=27&title=Nexitally&icon=bolt.horizontal.icloud.fill&color=#008000

# 机场流量信息（ARK）
Sub_info_01 = type = generic, timeout=10, script-path=https://raw.githubusercontent.com/mieqq/mieqq/master/sub_info_panel.js, script-update-interval=0, argument=url=https%3A%2F%2Farkdy.com%2Flink%2FS66tnFXYlBEGZpWc%3Fsub%3D2&reset_day=23&expire=2022-03-23&title=ARK&icon=bolt.horizontal.icloud.fill&color=#008000


